    "#FIBONACCI SEQUENCE\n",
    "def fibonacci(n):\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    elif n == 2:\n",
    "        return 1\n",
    "    elif n > 2:\n",
    "        return fibonacci (n-1) + fibonacci(n-2)\n",
    "for n in range (1,11):\n",
    "    print(fibonacci(n))\n",
    "    \n"
   ]
